<p id="notice"><%= notice %></p>

<h1>Listing Encrypted Objects</h1>

<table>
  <thead>
    <tr>
      <th>Secret stuff</th>
      <th>Secret stuff2</th>
      <th>Tell this to the world</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encrypted_objects.each do |encrypted_object| %>
      <tr>
        <td><%= encrypted_object.secret_stuff %></td>
        <td><%= encrypted_object.secret_stuff2 %></td>
        <td><%= encrypted_object.tell_this_to_the_world %></td>
        <td><%= link_to 'Show', encrypted_object %></td>
        <td><%= link_to 'Edit', edit_encrypted_object_path(encrypted_object) %></td>
        <td><%= link_to 'Destroy', encrypted_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encrypted object', new_encrypted_object_path %>
